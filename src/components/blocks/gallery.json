    },
    "background": {
      "type": "enumeration",
      "required": false,
      "default": "Transparent",
      "enum": [
        "Transparent",
        "White",
        "Light grey",
        "Dark",
        "Primary",
        "Secondary"
      ]
    },
    "width": {
      "type": "string",
      "required": false
    },
    "columns": {
      "type": "integer",
      "required": false
    },
    "title_display": {
      "type": "enumeration",
      "required": false,
      "default": "h2",
      "enum": [
        "h1",
        "h2",
